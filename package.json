{
  "name": "nextjs-poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "bun start",
    "lint": "eslint .",
    "clean": "bun pm cache rm && rm -rf .next",
    "clean:all": "rm -rf node_modules & rm bun.lock* & bun clean",
    "docs": "typedoc",
    "test": "jest --testPathPattern=src/.*?.unit.test.ts",
    "test:int": "jest --testPathPattern=src/.*?.int.test.ts",
    "test:all": "jest --testPathPattern=src/.*?.test.ts",
    "tc": "tsc --noEmit",
    "ci": "bun run test && bun run lint && bun run tc",
    "supa:login": "npx supabase login",
    "supa:types": "npx supabase gen types typescript --project-id qbnbwrggbrbnrpwfhtcw --schema public > src/types/supabase.database.types.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@react-oauth/google": "^0.12.1",
    "@supabase/supabase-js": "^2.43.4",
    "@types/cookie": "^0.6.0",
    "@types/gapi": "^0.0.47",
    "axios": "^1.7.2",
    "bun": "^1.1.10",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "emotion": "^11.0.0",
    "express": "^4.19.2",
    "googleapis": "^137.1.0",
    "net": "^1.0.2",
    "next": "14.2.3",
    "oauth2-client": "^0.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-transition-group": "^4.4.5",
    "simple-oauth2": "^5.0.1",
    "supabase": "^1.172.2"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "ts-jest": "^29.1.4",
    "typescript": "^5"
  }
}
